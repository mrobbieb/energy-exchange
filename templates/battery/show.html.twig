{% extends 'base.html.twig' %}

{% block title %}Battery{% endblock %}

{% block body %}
    <h1>Battery</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ battery.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ battery.name }}</td>
            </tr>
            <tr>
                <th>UserId</th>
                <td>{{ battery.userId }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ battery.createdAt ? battery.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>BatteryBankId</th>
                <td>{{ battery.batteryBankId }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ battery.updatedAt ? battery.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_battery_index') }}">back to list</a>

    <a href="{{ path('app_battery_edit', {'id': battery.id}) }}">edit</a>

    {{ include('battery/_delete_form.html.twig') }}
{% endblock %}
